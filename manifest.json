{
  "manifest_version": 3,
  "name": "Dot Cat Walker",
  "version": "1.0.0",
  "description": "A tiny pixel cat walks around on every page.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/content.js"],
      "run_at": "document_idle"
    }
  ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["assets/cat.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
